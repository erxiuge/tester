<div class="form-group">
  <%= gift_fields.label "Gifts", class: "col-sm-2 control-label" %>
  <div class="col-sm-4">
	<%= gift_fields.text_field :name, class: "form-control" %>
  </div>
  <div class="col-sm-offset-1 col-sm-2">
	<%= link_to "Delete", '#' , class: "delete-gift", onclick: "delete_gifts(this)" %>
	<%= gift_fields.hidden_field :id unless gift_fields.object.new_record? %>
	<%= gift_fields.hidden_field :_destroy , class: "destroy-mark" %>
  </div>
</div>